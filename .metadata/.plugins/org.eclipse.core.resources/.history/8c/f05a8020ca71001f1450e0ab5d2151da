package com.example.snowman.configurations;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;

import com.example.snowman.entities.InvoiceEntity;
import com.example.snowman.entities.PalletEntity;

@Configuration
public class AppConfig {
    @Bean
    public InvoiceEntity invoiceDB() {
        return new InvoiceEntity();
    }
    
    @Bean
    public PalletEntity palletDB() {
        return new PalletEntity();
    }
    
}