package com.example.snowman.repositories;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.example.snowman.entities.GoodReceiptNoteEntity;

@Repository
public interface GoodReceiptNoteRepository extends JpaRepository<GoodReceiptNoteEntity, Long>{
	List<GoodReceiptNoteEntity> findByInvoiceNumber(String invoiceNumber);
}
